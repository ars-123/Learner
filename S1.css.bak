align-items:center;
position:relative;
}
#home{
	display:flex;
justify-content:center;
align-items:center;
	height:400px;}

#home:before{
	content:"";
	background:url('E:/Assignment/bg.jpg') no-repeat center/cover;
	height:400px;
	width:100%;
	opacity:0.8;
	z-index:-1;
	position:absolute;
}
#navbar a{ padding:12px 22px;
border-radius:12px;
color:black;
border:none;
text-decoration:none;
}
#navbar a:hover{color:white;
background-color:blue;}

#butn{
	border-radius:6px;
